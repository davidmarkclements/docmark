    "tape": "^2.13.4",
    "watchify": "^1.0.1"
  },
  "scripts": {
    "rewrite": "bin/rewrite-marked > gen/marked-mod.js",
    "update-tests": "cp -r third_party/marked/test test/chjj-marked/test",
